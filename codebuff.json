{
  "description": "Template configuration for this project. See https://www.codebuff.com/config for all options.",
  "startupProcesses": [
    {
      "name": "dev",
      "command": "npm start",
      "enabled": true,
      "stdoutFile": "logs/dev.log"
    }
  ],
  "fileChangeHooks": [
    {
      "name": "typecheck",
      "command": "npm run build:ts",
      "filePattern": "**/*.{ts,tsx,js,jsx}",
      "enabled": true
    }
  ]
}